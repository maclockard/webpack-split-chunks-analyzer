{
  "name": "webpack-split-chunks-analyzer",
  "version": "0.2.8",
  "description": "Debug how your application is dynamically loaded by webpack",
  "keywords": [
    "webpack",
    "webpackplugin",
    "webpack-plugin",
    "webpackanalyzer",
    "splitchunks",
    "bundlesize"
  ],
  "url": "https://github.com/maclockard/webpack-split-chunks-analyzer",
  "license": "MIT",
  "author": "Mac Lockard (https://github.com/maclockard)",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "package.json",
    "README.md",
    "LICENSE",
    "lib",
    "dist",
    "src"
  ],
  "engines": {
    "node": ">=18.0.0"
  },
  "lint-staged": {
    "./**/*.{ts,js,json,md,html}": [
      "prettier --write"
    ]
  },
  "scripts": {
    "build": "npm-run-all build:plugin build:dist",
    "build:plugin": "tsc",
    "watch:plugin": "pnpm build -w",
    "build:dist": "NODE_ENV=production webpack-cli --progress --config webpack.config.js && rm dist/main.js",
    "watch:dist": "webpack-dev-server --progress --config webpack.config.js",
    "clean": "rm -rf lib dist node_modules"
  },
  "peerDependencies": {
    "webpack": "^5.0.0"
  },
  "dependencies": {
    "cheerio": "~0.22.0",
    "elkjs": "~0.7.1",
    "pretty-bytes": "^5.6.0",
    "runtypes": "^6.5.1",
    "transducist": "^2.2.0",
    "tslib": "^2.3.1"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@types/cheerio": "^0.22.31",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.3",
    "@types/webpack": "^5.28.0",
    "babel-loader": "^8.2.4",
    "core-js": "^3.21.1",
    "html-webpack-plugin": "^5.5.0",
    "lint-staged": "^12.3.7",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.6.2",
    "react": "^17.0.2",
    "react-dev-utils": "^12.0.0",
    "react-dom": "^17.0.2",
    "react-flow-renderer": "^10.0.8",
    "typescript": "^4.6.3",
    "webpack": "^5.71.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.1"
  }
}
